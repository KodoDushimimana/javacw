SET DATABASE COLLATION "Latin1_General"
CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE CACHED TABLE "ersStudentReg"("Registration ID" INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,"Student Name" VARCHAR(50),"Faculty" VARCHAR(50),"Project Title" VARCHAR(50),"Contact Number" INTEGER,"Email Address" ARCHAR(50),"project prototype" LONGVARBINARY)
SET TABLE "ersStudentReg" INDEX'32 1'
ALTER TABLE "ersStudentReg" ALTER COLUMN "Registration ID" RESTART WITH 1
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 60
